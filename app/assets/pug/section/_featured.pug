section.s-featured.m-works
  .m-works__wrap
    .m-works__header
      h1.m-works__title(data-inview="short") Selected Projects

    .m-works__loop
      each item, index in ["IF2MIzP2wXI", "Qe_dS4-jO6E", "HmREZuu4XuY", "asZvnGnpL7Q"]
        a.s-featured__item.o-work(href="case-study", data-inview="normal")
          .o-work__holder
            img.o-work__image(src="http://source.unsplash.com/" + item + "/16x10", data-src="http://source.unsplash.com/" + item + "/800x500", data-srcset="http://source.unsplash.com/" + item + "/800x500 800w, http://source.unsplash.com/" + item + "/1440x900 1440w", sizes="(min-width: 500px) 50vw, 100vw", alt="Featured image")
          .o-work__info
            strong.o-work__name
              faker-name-title/
            p.o-work__description
              +dummy(80, "chars")
              | &ensp;
            if index % 2 == 0
              mark.o-work__hint Visit Site
                include ../../svg/caret-right.svg
            else
              mark.o-work__hint Read Success Story
                include ../../svg/caret-right.svg

    .m-partners
      h2.m-partners__title(data-inview="short") Few of the brands weâ€™ve worked with
      .m-partners__list
        each logo in ["heineken", "hrabal", "jarosov", "lays", "lipton", "martini", "mnm", "monster", "lays", "lipton", "martini", "mnm", "heineken", "hrabal", "jarosov", "lays", "lipton", "martini"]
          .m-partners__item(data-inview="short")
            img.m-partners__image(src="assets/images/temp/logo-" + logo + ".svg", alt="Logo")

    a.m-works__button(href="our-work") View More Projects
