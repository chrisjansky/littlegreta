{
  "path": "./gulp/gulpconfig.json",
  "dev": {
    "root": "app/",

    "pagesFormat": ".html",
    "pagesFilter": "app/**/[^_]*.html",

    "templatesRoot": "app/assets/pug/",
    "templatesPartial": "/assets/pug",
    "templatesFilter": "app/assets/pug/**/[^_]*.pug",
    "templatesGlob": "app/assets/pug/**/*.pug",

    "dataGlob": "app/assets/data/**/*.json",
    "dataComb": "app/assets/pug/data/",

    "stylesRoot": "app/assets/scss/",
    "stylesBase": "app/assets/scss/*.scss",
    "stylesGlob": "app/assets/scss/**/*.scss",

    "tempRoot": "app/assets/temp/",

    "imagesGlob": "app/assets/images/**/*",

    "svgSourceGlob": "app/assets/svg/_source/*.svg",
    "svgBuildGlob": "app/assets/svg/*.svg",
    "svgBuild": "app/assets/svg/",

    "jsBundle": "app/assets/js/app.js"
  },
  "dist": {
    "root": "dist/"
  },
  "move": [
    "app/assets/svg/*.svg",
    "app/assets/video/*"
  ],
  "assets": [
    "node_modules/normalize.css/normalize.css",
    "app/assets/temp/style.css",

    "node_modules/headroom.js/dist/headroom.js",
    "node_modules/@vimeo/player/dist/player.js",
    "node_modules/in-view/dist/in-view.min.js",
    "node_modules/vanilla-lazyload/dist/lazyload.iife.js",
    "node_modules/barba.js/dist/barba.js",
    "node_modules/pace-progress/pace.js",
    "node_modules/two.js/build/two.js",
    "node_modules/waypoints/lib/noframework.waypoints.js",
    "node_modules/waypoints/lib/shortcuts/inview.js",

    "app/assets/temp/bundle.js"
  ]
}
